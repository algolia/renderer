{
  "name": "@algolia/renderscript",
  "version": "1.3.5",
  "description": "A custom JavaScript rendering engine based on puppeteer",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn clean && ttsc",
    "clean": "rm -rf dist/",
    "dev": "nodemon",
    "dev:run": "yarn build && NODE_ENV=development node dist/index.js",
    "docker:install": "node dist/lib/install.js",
    "docker:build": "./scripts/build.sh",
    "docker:release": "docker push algolia/renderscript",
    "release": "yarn docker:build && yarn docker:release",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/algolia/renderscript.git"
  },
  "keywords": [
    "algolia",
    "puppeteer",
    "js renderer",
    "rendertron",
    "prerender"
  ],
  "author": "Algolia <contact@algolia.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algolia/renderscript/issues"
  },
  "homepage": "https://github.com/algolia/renderscript#readme",
  "devDependencies": {
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@types/express": "4.17.2",
    "@types/node": "12.12.12",
    "@types/node-fetch": "2.5.4",
    "@types/puppeteer-core": "1.9.0",
    "@types/uuid": "3.4.6",
    "@types/yauzl-promise": "2.1.0",
    "concurrently": "5.0.0",
    "dotenv-safe": "8.2.0",
    "nodemon": "2.0.1",
    "prettier": "1.19.1",
    "semantic-release": "15.13.31",
    "tslint": "5.20.1",
    "ttypescript": "1.5.8",
    "typescript": "3.7.2",
    "typescript-transform-paths": "1.1.13"
  },
  "dependencies": {
    "@algolia/dns-filter": "1.1.3",
    "ad-block": "4.1.7",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "node-fetch": "2.6.0",
    "puppeteer-core": "2.0.0",
    "stream-to-string": "1.2.0",
    "uuid": "3.3.3",
    "yauzl-promise": "2.1.3"
  }
}
